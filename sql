CREATE TABLE card (
  id int AUTO_INCREMENT PRIMARY KEY,
  name varchar(255),
  clan varchar(255),
  lvl varchar(255),
  image varchar(255),
  imageSize varchar(255),
  ability varchar(255),
  bonus varchar(255),
  Ppower varchar(255),
  damage varchar(255),
  rarity varchar(255),
  top varchar(255),
  Left varchar(255),
  prismatic varchar(255)
);
